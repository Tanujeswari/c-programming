#include <stdio.h>
int main()
{
	int age;
	int b=0,s=0,a=0;
	int count=0;
	while(count<20)
	{
		printf("Enter age of person [%d]: ",count+1);
		scanf("%d",&age);
		
		if(age>=0 && age<=5)
			b++;
		else if(age>=6 && age<=17)
			s++;
		else
			a++;
		//increase counter
		count++;
	}
	
	printf("Baby age: %d\n",b);
	printf("School age: %d\n",s);
	printf("Adult age: %d\n",a);
	return 0;
}
